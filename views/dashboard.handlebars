{{> user}}
<h1>Dashboard</h1>
<button id="new-post-btn">New Post</button>
{{#each posts}}
  <div>
    <h2>{{this.title}}</h2>
    <p>{{this.content}}</p>
    <button data-id="{{this.id}}" class="edit-post-btn">Edit</button>
    <button data-id="{{this.id}}" class="delete-post-btn">Delete</button>
  </div>
{{/each}}
